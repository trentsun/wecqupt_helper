/* pages/home.wxss */


.search-wrapper {
  display: flex;
  flex-direction: row;
  background: #e5e5e5;
  border-radius: 100rpx;
  margin-left: 20rpx;
  align-items: center;
  z-index: 100;
}

.add-icon {
  position: fixed;
  width: 20%;
  height: 10%;
  left: 40%;
  top: 3%;
}

.clear {
  position: fixed;
  width: 20%;
  height: 10%;
  right: 3%;
  top: 3%;
}

.background {
  width: 100%;
  height: 100%;
  position: absolute;
}